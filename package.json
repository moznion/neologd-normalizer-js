{
  "name": "neologd-normalizer",
  "version": "0.0.1",
  "description": "neologd normalizing script for JavaScript",
  "main": "build/index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register",
    "lint": "eslint ./index.es",
    "check": "npm test && npm run lint",
    "clean": "rm build/*",
    "build": "npm run clean && browserify index.es -o build/index.js -t babelify",
    "prepublish": "npm run build",
    "bench": "browserify ./author/bench.es -o ./author/bench.js -t babelify && node ./author/bench.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moznion/neologd-normalizer-js.git"
  },
  "keywords": [
    "regexp",
    "neologd"
  ],
  "dependencies": {
    "moji": "^0.5.1"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.18.0",
    "babelify": "^7.3.0",
    "benchmark": "^2.1.2",
    "browserify": "^13.1.1",
    "eslint": "^3.12.0",
    "mocha": "^3.2.0",
    "power-assert": "^1.4.2"
  },
  "author": "moznion",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/moznion/neologd-normalizer-js/issues"
  },
  "homepage": "https://github.com/moznion/neologd-normalizer-js"
}
